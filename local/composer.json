{
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "respect/validation": "^2.3",
    "google/recaptcha": "^1.3"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "classes\\": [
        "../local/php_interface/classes/"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
